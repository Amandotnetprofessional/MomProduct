@model IEnumerable<MomProduct.Web.Models.BabyName>

<!DOCTYPE html>
<html>
<head>
    <title>100 Modern Indian Baby Boy Names</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        h1 {
            text-align: center;
            font-size: 24px;
        }

        table {
            width: 80%;
            margin: 20px auto;
            border-collapse: collapse;
        }

        th, td {
            text-align: left;
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

        th {
            font-size: 18px;
        }

        tr:hover {
            background-color: #f1f1f1;
        }

        a {
            color: #007BFF;
            text-decoration: none;
        }

            a:hover {
                text-decoration: underline;
            }
    </style>
</head>
<body>
    <h1>100 Modern Indian Baby Boy Names</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Meaning</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var babyName in Model)
            {
                <tr>
                    <td><a href="#">@babyName.Name</a></td>
                    <td>@babyName.Meaning</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
